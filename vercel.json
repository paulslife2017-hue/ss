{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "X-Robots-Tag": "index, follow",
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    },
    {
      "src": "/",
      "headers": {
        "Link": "<https://seoulzen.com/>; rel=\"canonical\""
      },
      "continue": true
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
