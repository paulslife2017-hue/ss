{
  "framework": null,
  "installCommand": "echo 'No install needed'",
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": ".",
  "devCommand": null,
  "redirects": [
    {
      "source": "https://www.seoulzen.com/:path*",
      "destination": "https://seoulzen.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Link",
          "value": "<https://seoulzen.com/>; rel=\"canonical\""
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}
